                                <sdk:Label Grid.Row="1" HorizontalAlignment="Left" Name="LB_2_1" VerticalAlignment="Top" Width="402" Margin="14,5,0,0" Content="1.選擇CL檔或NC檔：" Grid.ColumnSpan="2" />
