                                        <sdk:DataGrid.Columns>
                                            <sdk:DataGridCheckBoxColumn CanUserReorder="True" CanUserResize="True" CanUserSort="True" Header="選擇" Width="Auto" Binding="{Binding Machine_Select}"/>
